{
  "name": "plasmo-my-email-ai",
  "displayName": "My Email AI Assistant",
  "version": "0.0.1",
  "description": "AI-powered email assistant with voice commands",
  "author": "Myungeun Lee",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.11.0",
    "plasmo": "0.88.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.268",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "prettier": "3.3.1",
    "typescript": "5.4.5"
  },
  "manifest": {
    "host_permissions": [
      "http://localhost:3000/*",
      "https://mail.google.com/*",
      "https://outlook.office.com/*",
      "https://outlook.live.com/*"
    ],
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ]
  }
} 